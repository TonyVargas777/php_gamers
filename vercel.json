{
    "version": 2,    
    "routes": [
        {
            "src": "/(css|js)/(.*)",
            "dest": "img/$1/$2"
        },
        {
            "src": "/img/(.*)",
            "dest": "img/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}